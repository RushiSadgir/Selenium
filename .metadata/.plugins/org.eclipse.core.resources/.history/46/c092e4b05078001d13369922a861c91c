package studyOfPattern;

public class P1 {

	public static void main(String[] args) 
	{
		for(int i=1;i<=5;i++)
		{
			System.out.print("*");
		}
		System.out.println();
		System.out.println("================");
		for(int i=1;i<=5;i++)
		{
			System.out.println("*");
		}
		System.out.println();
		System.out.println("================");
		for(int i=1;i<=5;i++)
		{
			for(int j=1;j<=5;j++)
			{
				System.out.println("*");
			}
		}
	}

}
